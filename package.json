{
  "name": "kerplunk-database",
  "description": "",
  "version": "0.0.3",
  "kerplunk": {
    "services": {
      "mongo": {
        "private": true,
        "image": "mongo:3.0.4",
        "volumes": [
          "/data/db"
        ],
        "ports": [
          "27017/tcp"
        ],
        "cmd": [
          "mongod",
          "--smallfiles"
        ]
      }
    }
  },
  "keywords": [
    "kerplunk-plugin"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "main": "./lib/database.js",
  "dependencies": {
    "connect-mongo": "^0.8.0",
    "express-session": "^1.11.1",
    "mongoose": "^4.0.6"
  },
  "devDependencies": {
    "coffee-script": "^1.9.1",
    "glut": "0.0.1",
    "gulp": "^3.8.8",
    "gulp-coffee": "^2.3.1"
  },
  "scripts": {
    "prepublish": "rm -rf ./lib ./public && gulp"
  }
}
